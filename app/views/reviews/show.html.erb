<div class = "container text-center">
  <h1> SideQuest</h1>
  <%= @sidequest.name %>
  <%= @sidequest.address %><br>
  <% if @review %>
  <%= @review.body%>
  <%end%><br>
  <%= link_to 'Create my SideQuest', new_side_quest_path, class: 'btn btn-outline-success' %>
  <div class="footer"id="footer-info">
    <%= link_to 'Post', new_side_quest_review_path(@sidequest.id) %>
  </div>
</div>
