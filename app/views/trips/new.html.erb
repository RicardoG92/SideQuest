      <%= f.input :name, :label =>  "End Point"%>
      <div>
        <label class="form-label range optional" for="user_mood">Distance From Main Road</label>
        <input class="form-range numeric range optional" type="range" step="1" name="user[mood]" id="user_mood">
      </div>
        <%= f.fields_for :category do |category_subform| %>
          # fields for the associated category object
          <%= category_subform.text_field :category, :label =>  "Category" %>
